<%if flash[:errors]%>
    <%flash[:errors].each do |e|%>
        <%=e%>
    <%end%>
<%end%>

<%= form_for @artist do |f| %>
    <%=f.label :name%>
    <%=f.text_field :name%>

    <%=f.label :title%>
    <%=f.text_field :title%>

    <%=f.label :age%>
    <%=f.number_field :age%>

    <%=f.submit%>
<% end %>